<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/table_price_background"
    android:layout_marginStart="10dp"
    android:layout_height="wrap_content">

    <TableRow>
        <TextView
            android:layout_width="100dp"
            android:layout_marginStart="2dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:layout_column="1"
            android:text="Nhập khẩu"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />
        <TextView
            android:layout_width="100dp"
            android:id="@+id/tv_row_price_asia_air_stt"
            android:layout_marginStart="2dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:layout_column="1"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_asia_air_aol"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_asia_air_aod"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_asia_air_dim"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold">
        </TextView>

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_asia_air_grossweight"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="250dp"
            android:id="@+id/tv_row_price_asia_air_typeofcargo"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_asia_air_freight"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_asia_air_surcharge"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_asia_air_airlines"
            android:layout_marginStart="1dp"
            android:textColor="@color/black"
            android:gravity="center"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_asia_air_schedule"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_asia_air_transittime"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_asia_air_valid"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />
        <TextView
            android:layout_width="200dp"
            android:id="@+id/tv_row_price_asia_air_note"
            android:layout_marginStart="1dp"
            android:gravity="center"
            android:textColor="@color/black"
            android:paddingHorizontal="20dp"
            android:paddingVertical="10dp"
            android:textStyle="bold" />
    </TableRow>

</TableLayout>